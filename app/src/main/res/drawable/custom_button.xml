<?xml version="1.0" encoding="utf-8"?>



<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape>
            <gradient
                android:startColor="#FB5607"
                android:endColor="#FFBE0B"
                android:angle="180"
                />
            <stroke
                android:width="1dp"
                android:color="@color/teal_200"
                />

            <corners
                android:radius="4dp"/>

            <padding
                android:right="10dp"
                android:left="10dp"
                android:bottom="1dp"
                android:top="1dp"
                />

        </shape>
    </item>


</selector>